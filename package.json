{
  "name": "agora",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">18.0.0",
    "pnpm": "^7.0.0"
  },
  "scripts": {
    "lint": "npx nx run-many --target=lint --all --output-style=static",
    "lint-fix": "npm run lint -- --fix",
    "type-check": "npx nx run-many --target=type-check --all --output-style=static",
    "serve": "npx nx run-many --target=serve --all --output-style=stream",
    "serve:frontend": "npx nx run frontend:serve",
    "build:frontend": "npx nx run frontend:build",
    "test": "npx nx run-many --target=test --all --output-style=static",
    "test:coverage": "npx nx run-many --target=test --all --output-style=static -- --coverage",
    "generate": "npx nx run-many --target=generate --all",
    "cdk:backend:up": "cdk deploy --app \"ts-node --project ./apps/backend/tsconfig.json ./apps/backend/src/main.ts\" --all",
    "cdk:backend:down": "cdk destroy --app \"ts-node --project ./apps/backend/tsconfig.json ./apps/backend/src/main.ts\" --all",
    "cdk:backend:diff": "cdk diff --app \"ts-node --project ./apps/backend/tsconfig.json ./apps/backend/src/main.ts\" --all",
    "cdk:frontend:up": "npm run build:frontend && cdk deploy --app \"ts-node --project ./apps/frontend/cdk/tsconfig.json ./apps/frontend/cdk/deploy.ts\" --all",
    "cdk:frontend:down": "cdk destroy --app \"ts-node --project ./apps/frontend/cdk/tsconfig.json ./apps/frontend/cdk/deploy.ts\" --all",
    "cdk:frontend:diff": "cdk diff --app \"ts-node --project ./apps/frontend/cdk/tsconfig.json ./apps/frontend/cdk/deploy.ts\" --all"
  },
  "private": true,
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.51.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.51.1-alpha.0",
